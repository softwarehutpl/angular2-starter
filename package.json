{
  "name": "angular2-starter",
  "version": "0.1.0",
  "scripts": {
    "build": "webpack --progress",
    "build:prod": "webpack -p --progress",
    "postinstall": "typings install",
    "server": "webpack-dev-server --inline --progress --content-base=./src/",
    "backend": "json-server db.json",
    "dev": "concurrently --kill-others \"npm run backend\" \"npm run server\""
  },
