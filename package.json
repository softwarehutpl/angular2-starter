    "@angular/http": "2.4.1",
    "@angular/platform-browser": "2.4.1",
    "@angular/platform-browser-dynamic": "2.4.1",
    "@angular/router": "3.0.1",
    "bootstrap": "^4.0.0-alpha.6",
    "codelyzer": "^2.0.1",
    "concurrently": "^3.4.0",
    "core-js": "2.4.1",
    "jquery": "^2.2.3",
    "jquery-ui": "1.10.5",
    "json-server": "^0.9.6",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.0-beta.12",
